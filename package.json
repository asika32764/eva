{
    "name": "windwalker",
    "private": true,
    "scripts": {
        "build": "cross-env node_modules/.bin/gulp",
        "build:dev": "cross-env NODE_ENV=development node_modules/.bin/gulp",
        "build:prod": "cross-env NODE_ENV=production node_modules/.bin/gulp",
        "watch": "cross-env NODE_ENV=development node_modules/.bin/gulp --watch",
        "watch:reload": "cross-env NODE_ENV=development node_modules/.bin/gulp --watch --livereload",
        "postinstall": "yarn build install"
    },
    "dependencies": {
        "@windwalker-io/fusion": "^1.0.0-beta",
        "alpinejs": "^2.8.2",
        "bootstrap": "^5.0.0-beta"
    },
    "resolutions": {
        "graceful-fs": "^4.2.4"
    },
    "workspaces": [
        "vendor/windwalker/unicorn/*"
    ]
}
