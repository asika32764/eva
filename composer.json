{
    "name": "windwalker/starter",
    "description": "Windwalker Starter.",
    "homepage": "https://github.com/ventoviro/windwalker-starter",
    "license": "MIT",
    "minimum-stability": "beta",
    "authors": [
        {
            "name": "Simon Asika",
            "email": "asika32764@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-json": "*",
        "windwalker/core": "4.x-dev",
        "windwalker/framework": "4.x-dev",
        "symfony/dotenv": "^5.0",
        "nikic/fast-route": "^1.0",
        "monolog/monolog": "^2.0",
        "relay/relay": "^2.0",
        "filp/whoops": "^2.1",
        "symfony/var-dumper": "^5.2",
        "symfony/options-resolver": "^5.0",
        "symfony/console": "^5.2",
        "fakerphp/faker": "^1.0",
        "symfony/process": "^5.2",
        "windwalker/unicorn": "dev-main",
        "myclabs/php-enum": "^1.8",
        "jenssegers/agent": "^2.6"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.0",
        "dragonmantank/cron-expression": "^3.1",
        "symfony/mailer": "^5.2",
        "composer-plugin-api": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "scripts": {
    },
    "config": {
        "platform": {
            "php": "8.0.0"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.x-dev"
        }
    }
}
